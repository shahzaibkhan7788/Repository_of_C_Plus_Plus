	   printf("%d\t",A[i]);
	}
	printf("Reverse order of matrix\n");
	for(int i=9;i>=0;i--)
	{
	   B[i]=A[9-i];
	}
	printf("\n");
	for(int i=0;i<10;i++)
	{
		printf("%d\t",B[i]);
	}

